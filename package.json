{
  "name": "vue-game-card",
  "private": true,
  "version": "0.4.0",
  "license": "MIT",
  "description": "Vue game card component for building card games",
  "author": "Maxim Kosterin <maxstoneg@gmail.com>",
  "type": "module",
  "scripts": {
    "build": "vite build",
    "release": "semantic-release"
  },
  "files": [
    "dist"
  ],
  "main": "./dist/vue-game-card.umd.cjs",
  "module": "./dist/vue-game-card.js",
  "exports": {
    ".": {
      "import": "./dist/vue-game-card.js",
      "require": "./dist/vue-game-card.umd.cjs"
    }
  },
  "release": {
    "branches": [
      "main"
    ],
    "plugins": [
      "@semantic-release/commit-analyzer",
      "@semantic-release/release-notes-generator",
      "@semantic-release/npm"
    ]
  },
  "dependencies": {
    "core-js": "3.26.1",
    "vue": "^3.2.45"
  },
  "devDependencies": {
    "@rollup/plugin-alias": "^4.0.2",
    "@vitejs/plugin-vue": "^4.0.0",
    "sass": "^1.57.1",
    "typescript": "^4.9.3",
    "vite": "^4.0.0",
    "vue-tsc": "^1.0.11",
    "semantic-release": "19.0.5"
  }
}
